{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "plugin:prettier/recommended",
    "eslint:recommended"
  ],
  "rules": {
    "prettier/prettier": [1, { "endOfLine": "auto" }],
    "@typescript-eslint/no-var-requires": 0
  }
}
